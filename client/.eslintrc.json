{
  "extends": ["next/core-web-vitals", "next/babel"],
  "rules": {
    // Unused Variables
    "no-unused-vars": "error",

    // Naming Convention
    "@typescript-eslint/naming-convention": [
      "error",
      {
        "selector": "variable",
        "format": ["PascalCase", "camelCase"]
      },
      {
        "selector": "interface",
        "format": ["StrictPascalCase"],
        "prefix": ["I"]
      }
    ]
  }
}
